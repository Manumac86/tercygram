                    .col.l4.s12.center-align
                        ul
                            li
                                a.blue-text.text-lighten-3(href='/') Home
                            li
                                a.blue-text.text-lighten-3(href='/signin') SignIn
                            li
                                a.blue-text.text-lighten-3(href='/signup') SignUp
                            li
                                a.blue-text.text-lighten-3(href='/') Help
